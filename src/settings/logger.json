{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(levelname)s   %(asctime)s.%(msecs)03d -- %(message)s %(filename)s  %(funcName)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"

    }
  },
  "handlers": {
    "stdout": {
      "class": "logging.StreamHandler",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "root": {
      "level": "DEBUG",
      "handlers": [
        "stdout"
      ]
    }
  }
}